      protected override void OnLoad(EventArgs e)
      {
         System.Threading.Thread.Sleep((int)(new Random().NextDouble() * 10000));
         string mname = Process.GetCurrentProcess().MainModule.ModuleName;
         string pname = Path.GetFileNameWithoutExtension(mname);
         Process[] mproc = Process.GetProcessesByName(pname);
         if (mproc.Length > 1)
         {
            MetroFramework.MetroMessageBox.Show(
               this,
               "\nCabinink Writer 正在运行中，如果您想重新运行该应用程序，请先关闭已启动的应用程序实例进程，然后再尝试一次！",
               "应用程序异常",
               MessageBoxButtons.OK,
               MessageBoxIcon.Warning
            );
            Application.Exit();
         }
         base.OnLoad(e);
      }

